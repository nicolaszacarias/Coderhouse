<div>
    <h1>Home</h1>
    <h2>Lista de Productos</h2>
    <a href="/carts/{{this._id}}">Ver Carrito</a>
    <ul>
        {{#each products}}
    <h2>{{this.title}}</h2>
    <h3>Precio: ${{this.price}}</h3>
    <p>{{this.description}}</p>
    <p>Categoría: {{this.category}}</p>
    <a href="/products/{{this._id}}">Ver descripción</a>
        {{/each}}
    </ul>

</div>